<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qc.business.mapper.OutportMapper">

	<!-- queryOutportCountByInportId -->
	<select id="queryOutportCountByInportId" resultType="java.lang.Integer">
		select count(1) from bus_outport where inportid=#{value}
	</select>

</mapper>
